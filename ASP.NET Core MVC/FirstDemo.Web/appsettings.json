{

  "ConnectionStrings": {
    "DefaultConnection": "Server = .\\SQLEXPRESS; Database = Asp.netCoreMvc; User Id = Asp.netCoreMvc; Password = 123456;"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/web-log-.log",     //// path indicate where log file be created 
          "rollingInterval": "Day"
        }
      }
    ]
  },

  "AllowedHosts": "*"
}
